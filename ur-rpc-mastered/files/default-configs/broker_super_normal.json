{
  "network": {
    "bind_address": "0.0.0.0",
    "port": 1885,
    "max_clients": 100,
    "max_message_size": 16384,
    "keep_alive_interval": 60
  },
  "ssl": {
    "enabled": false
  },
  "broker": {
    "max_inflight_messages": 50,
    "max_queued_messages": 100,
    "retain_available": true,
    "wildcard_subscriptions": true
  },
  "authentication": {
    "allow_anonymous": true
  }
}

